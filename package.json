{
  "name": "vision-car",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "resources:start": "docker compose up dev-db redis redis-commander -d",
    "resources:stop": "docker compose rm dev-db redis redis-commander -s -f -v",
    "resources:restart": "yarn resources:stop && yarn resources:start",
    "db:dev:start": "docker compose up dev-db -d",
    "db:dev:stop": "docker compose rm dev-db -s -f -v",
    "db:dev:restart": "yarn db:dev:stop && yarn db:dev:start",
    "prisma:dev:deploy": "npx prisma migrate deploy",
    "prisma:dev:studio": "npx prisma studio",
    "prisma:dev:display": "yarn prisma:dev:deploy && yarn prisma:dev:studio",
    "db:test:start": "docker compose up test-db -d && sleep 1 && yarn prisma:test:deploy",
    "db:test:stop": "docker compose rm test-db -s -f -v",
    "db:test:restart": "yarn db:test:stop && yarn db:test:start",
    "prisma:test:deploy": "dotenv -e .env.test -- npx prisma migrate deploy",
    "prisma:test:studio": "dotenv -e .env.test -- npx prisma studio",
    "redis:cli": "docker exec -it vision-car-server-redis-1 redis-cli",
    "postgres:cli": "docker exec -it vision-car-server-dev-db-1 bash",
    "format": "npx prisma format",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --no-cache --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "^4.10.1",
    "argon2": "^0.30.3",
    "cache-manager": "^5.1.6",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^6.1.3",
    "dotenv": "^16.0.3",
    "express-session": "^1.17.3",
    "global": "^4.4.0",
    "mailgen": "^2.0.27",
    "nodemailer": "^6.9.1",
    "pactum": "^3.3.2",
    "redis": "^4.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "prisma": "^4.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
